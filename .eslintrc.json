{
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "surseType": "module"
  },
  "globals": {
    "AppError": true
  },
  "extends": ["eslint:recommended", "google"],
  "rules": {
    "arrow-parens": "off",
    "no-console": 0,
    "object-curly-spacing": 0,
    "max-len": 0,
    "require-jsdoc": 0,
    "no-invalid-this": 0
  }
}
